//测试函数定义语句
void f1(int num){printf("%d ");} //测试函数声明以及函数使用
void f2(int num){//测试函数定义 funcdcl
    f1(num);
    if (num == 10){
        putchar('\n');
        return ;
    }
    f2(num+1);
}
int f3(int p1,int p2,float p3){
    f2(p1);
    f2(p2);
    f2(p3);
    return 0;
}
int main(){
    return f3(1,2,3);
}
